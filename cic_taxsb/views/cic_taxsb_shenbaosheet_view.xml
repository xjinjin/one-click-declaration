<?xml version="1.0" encoding="utf-8" ?>

<odoo>
    <record id="view_tree_shenbaosheet" model="ir.ui.view">
         <field name="name">shenbaosheet Tree</field>
         <field name="model">cic_taxsb.shenbaosheet</field>
         <field name="arch" type="xml">
             <tree>
                 <field name="name" />
                 <field name="description" />
                 <field name="dqbm" />
                 <field name="tagname" />
             </tree>
         </field>
    </record>


    <record id="view_form_shenbaosheet" model="ir.ui.view">
         <field name="name">申报表模板</field>
         <field name="model">cic_taxsb.shenbaosheet</field>
         <field name="arch" type="xml">
             <form string="shenbaosheet">
                 <sheet>
                    <group>
                        <group>
                            <field name="name" />
                            <field name="dqbm" />
                            <field name="tagname" />
                            <field name="parent_id" />
                        </group>
                        <group>
                            <field name="sequence" />
                            <field name="description" />
                            <field name="template" />
                        </group>
                    </group>
                    <group><field name="cells" /></group>
                 </sheet>
             </form>
         </field>
    </record>
</odoo>